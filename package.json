{
  "name": "landing-perf-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "http-server ./dist/landing-perf-angular -p 8080",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/landing-perf-angular/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lhci": "npm run build && lhci autorun --config=lighthouserc.js",
    "lhci:ssr": "npm run build:ssr && lhci autorun --config=lighthouserc-ssr.js",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.1.1",
    "@angular/common": "15.1.1",
    "@angular/compiler": "15.1.1",
    "@angular/core": "15.1.1",
    "@angular/forms": "15.1.1",
    "@angular/platform-browser": "15.1.1",
    "@angular/platform-browser-dynamic": "15.1.1",
    "@angular/router": "15.1.1",
    "@fortawesome/angular-fontawesome": "0.11.1",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "chart.js": "3.8.0",
    "ng2-charts": "3.1.2",
    "rxjs": "7.5.0",
    "swiper": "8.3.0",
    "timeago.js": "^4.0.2",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "15.1.2",
    "@angular/compiler-cli": "15.1.1",
    "@lhci/cli": "^0.10.0",
    "@types/jasmine": "4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "http-server": "14.1.1",
    "jasmine-core": "4.1.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "typescript": "4.9.4",
    "webpack-bundle-analyzer": "4.5.0"
  }
}
